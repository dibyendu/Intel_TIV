(define (problem mmu_deadlock_case_study_1)
(:domain arm_domain)
(:objects x y - data)
(:init (idle_dram_addr x)
	(not (cExpire_dram_addr x))
	(not (cke))
	(d_cache_avail)
	(not (valid_addr y))
	(valid_addr x)
	(busIdle)
	(DMA_descriptor)
	(DMA_En)
	(Device_En)
)

(:goal (and (cke)
	(refresh_addr x)
	(writeReq x)
	(blockBus))
)
)
